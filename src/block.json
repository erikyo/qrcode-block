{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blocchi/qr-code",
	"version": "0.1.0",
	"title": "QR-code block",
	"icon": "feedback",
	"category": "widgets",
	"textdomain": "barcode",
	"attributes": {
		"content": {
			"type": "string",
			"default": "QR-Code text"
		},
		"size": {
			"type": "number",
			"default": 100
		},
		"errorCorrection": {
			"type": "string",
			"default": "L"
		},
		"textColor": {
			"type": "object",
			"default": {
				"color": "#000000"
			}
		},
		"backgroundColor": {
			"type": "object",
			"default": {
				"color": "#ffffff"
			}
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"shadow": true,
		"dimensions": {
			"minHeight": true
		}
	},
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}
